{
  "framework": "nextjs",
  "buildCommand": "next build",
  "outputDirectory": ".next",
  "crons": [
    {
      "path": "/api/cron/pickup-reminders",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron/daily-report",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/cron/follow-up",
      "schedule": "0 10 * * 1"
    }
  ]
}
